{
  "name": "cache-first",
  "author": "Alex Reardon <alexreardon@gmail.com>",
  "license": "MIT",
  "version": "0.1.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/alexreardon/cache-first.git"
  },
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "build": "bun build ./src/cache-first.ts --outdir ./dist --format esm",
    "build:types": "tsc ./src/cache-first.ts --outDir ./dist --declaration --emitDeclarationOnly --skipLibCheck",
    "prettier:validate": "prettier --debug-check src/**/*.ts test/**/*.ts"
  },
  "exports": {
    ".": "./dist/cache-first.js",
    "types": "./dist/cache-first.d.ts"
  },
  "engines": {
    "bun": ">=1.1.8"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "prettier": "^3.2.5"
  },
  "peerDependencies": {
    "typescript": "^5.4.5"
  }
}
