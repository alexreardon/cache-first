{
  "name": "memoize-first",
  "author": "Alex Reardon <alexreardon@gmail.com>",
  "license": "MIT",
  "version": "0.1.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/alexreardon/memoize-first.git"
  },
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "prepublishOnly": "bun build:clean && bun build:dist && bun build:types",
    "build:clean": "rimraf dist",
    "build:dist": "bun build ./src/memoize-first.ts --outdir ./dist --format esm",
    "build:types": "tsc ./src/memoize-first.ts --outDir ./dist --declaration --emitDeclarationOnly --skipLibCheck",
    "prettier:validate": "prettier --debug-check src/**/*.ts test/**/*.ts"
  },
  "exports": {
    ".": "./dist/memoize-first.js",
    "./types": "./dist/memoize-first.d.ts"
  },
  "files": [
    "/dist",
    "/src"
  ],
  "engines": {
    "bun": ">=1.1.8"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7"
  },
  "peerDependencies": {
    "typescript": "^5.4.5"
  }
}
