{
  "name": "limit-once",
  "description": "Remember the first result of a function call",
  "version": "0.14.0",
  "author": "Alex Reardon <alexreardon@gmail.com>",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/alexreardon/limit-once.git"
  },
  "keywords": [
    "once",
    "cache",
    "limit",
    "performance",
    "memoize",
    "memoization"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "exports": {
    "types": {
      "import": "./dist/index.d.ts",
      "require": "./dist/index.d.cts"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "/dist",
    "/src"
  ],
  "scripts": {
    "prepublishOnly": "bun build:dist",
    "build:dist": "bun build:clean && bun tsup",
    "build:clean": "rimraf ./dist",
    "check:all": "bun check:prettier && bun check:typescript && bun check:dist",
    "check:prettier": "prettier --debug-check src/**/*.ts test/**/*.ts",
    "check:typescript": "tsc",
    "check:dist": "attw --pack ."
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.15.3",
    "@types/bun": "latest",
    "expect-type": "^0.19.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "tiny-invariant": "^1.3.3",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  }
}
